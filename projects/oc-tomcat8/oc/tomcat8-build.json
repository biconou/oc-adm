{
  "kind": "BuildConfig",
    "apiVersion": "v1",
    "metadata": {
      "name": "tomcat8-build"
    },
  "spec": {
    "source": {
      "type": "Binary"
    },
    "strategy": {
      "type": "Docker"
    },
    "output": {
      "to": {
        "kind": "ImageStreamTag",
        "name": "tomcat8:latest"
      }
    }
  }
}
